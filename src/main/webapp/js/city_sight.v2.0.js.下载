function getDateStr(a,c){var b=new Date;if(c)var b=c.split("-"),e=parseInt(b[0],10),d=parseInt(b[1],10)-1,b=parseInt(b[2],10),b=new Date(e,d,b);b.setDate(b.getDate()+a);e=b.getFullYear();d=10>b.getMonth()+1?"0"+(b.getMonth()+1):b.getMonth()+1;b=10>b.getDate()?"0"+b.getDate():b.getDate();return e+"-"+d+"-"+b}
function checkDate(a,c){var b=a.split("-"),e=c.split("-"),d=parseInt(b[0],10),f=parseInt(e[0],10),g=parseInt(b[1],10),k=parseInt(e[1],10),b=parseInt(b[2],10),e=parseInt(e[2],10);return d<f||d==f&&g<k||d==f&&g==k&&b<e?!0:!1}function GetDateDiff(a){var c=new Date;a=new Date(Date.parse(a.replace(/-/g,"/")));return Math.ceil(parseFloat((a-c)/864E5,10))}
